a, b, c = 15, 14, 7
if a == b == c : 
    print("Variable Ã©gales")
elif a % b == 0 or b % c == 0 or c % a == 0 : 
    print("L'une des trois variable en divise une autre")
else:
    print("Aucune de ces variable n'en disive une autre")

n = int(input("Entrer un nombre entier pas trop grand stp"))
liste, somme = [], 0
for i in range(n):
    liste.append(int(input("Entrer un nombre entier")))
for a in range(n):
    somme += liste[a]
print(somme)

